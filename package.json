{
  "name": "dev",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "style:lint": "stylelint \"./src/**/*.scss\" --config ./config/.stylelintrc.json",
    "lint": "npm run style:lint -- --fix",
    "image:min": "node ./config/image.min.mjs",
    "remove-comments": "node ./config/remove-comments.mjs",
    "dev": "npm run lint && vite --config ./config/vite.config.js && npm run image:min",
    "build": "npm run lint && vite build --config ./config/vite.config.js && npm run remove-comments"
  },
  "devDependencies": {
    "@macropygia/vite-plugin-pug-static": "^0.0.19",
    "autoprefixer": "^10.4.20",
    "css-declaration-sorter": "^7.2.0",
    "globule": "^1.3.4",
    "postcss-sort-media-queries": "^5.2.0",
    "pug-plugin": "^5.3.0",
    "rollup-plugin-license": "^3.5.2",
    "sass": "^1.75.0",
    "sharp": "^0.33.5",
    "stylelint": "^16.8.2",
    "stylelint-config-property-sort-order-smacss": "^10.0.0",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.2",
    "stylelint-scss": "^6.5.0",
    "vite": "^5.4.2",
    "vite-plugin-static-copy": "^1.0.6"
  },
  "dependencies": {
    "gsap": "^3.12.5",
    "swiper": "^8.4.7",
    "three": "^0.169.0",
    "vite-plugin-glsl": "^1.3.0"
  }
}
